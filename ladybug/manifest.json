{
  "name": "Miraculous Tab",
  "description": "Miraculous Ladybug tab!",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "images/ladybug-icon.png"
  },
  "permissions": [
         "storage"
     ],
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },

  "content_security_policy": "script-src blob: filesystem: chrome-extension-resource: 'self' 'unsafe-eval'; object-src 'self'"
}